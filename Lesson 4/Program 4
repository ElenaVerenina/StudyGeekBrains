using System;

namespace Lesson_4_4
{
    class Program
    {
        static int Fibonacci (int Number)
        {
            int Fib = 1;
            if (Number == 1 || Number == 2)
            {
                return Fib = 1;
            }
            else return Fib = Fibonacci(Number - 1) + Fibonacci(Number - 2);

        }
        static void Main(string[] args)
        {
            int Fib = 0;
            Console.WriteLine("Please, enter Number Fibonacci");
            Fib = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine($"Fibonacci: {Fibonacci(Fib)}");
        }
    }
}
